From f4c371cca4be692791048b66464921103c7f6f06 Mon Sep 17 00:00:00 2001
From: Joseph Liu <kwliu@nuvoton.com>
Date: Tue, 14 Mar 2023 09:40:44 +0800
Subject: [PATCH 2/2] dts: evb-npcm845: add more reserved memory

Signed-off-by: Joseph Liu <kwliu@nuvoton.com>
---
 .../boot/dts/nuvoton/nuvoton-npcm845-evb.dts  | 24 +++++++++++++++++++
 1 file changed, 24 insertions(+)

diff --git a/arch/arm64/boot/dts/nuvoton/nuvoton-npcm845-evb.dts b/arch/arm64/boot/dts/nuvoton/nuvoton-npcm845-evb.dts
index 5dc897741deb..b8e2e99082f0 100644
--- a/arch/arm64/boot/dts/nuvoton/nuvoton-npcm845-evb.dts
+++ b/arch/arm64/boot/dts/nuvoton/nuvoton-npcm845-evb.dts
@@ -78,6 +78,30 @@ reserved-memory {
 		#size-cells = <2>;
 		ranges;
 
+		gfx_memory: gfx@37000000 {
+			reg = <0x0 0x37000000 0x0 0x1000000>;
+			no-map;
+		};
+
+		optee_memory: optee@36000000 {
+			reg = <0x0 0x36000000 0x0 0x1000000>;
+			no-map;
+		};
+
+		tcg_log: tcg-log@35F00000 {
+			reg = <0x0 0x35F00000 0x0 0x10000>;
+		};
+
+		tip_memory: tip@34f00000 {
+			reg = <0x0 0x34f00000 0x0 0x1000000>;
+			no-map;
+		};
+
+		cp_memory: cp@34d00000 {
+			reg = <0x0 0x34d00000 0x0 0x20000>;
+			no-map;
+		};
+
 		video_memory: framebuffer@33000000 {
 			compatible = "shared-dma-pool";
 			reg = <0x0 0x33000000 0x0 0x01800000>;
-- 
