     watchdog2 = 0x23,
     entity = 0x25,
     oemC0 = 0xc0,
+    subsystem_health = 0xE0,
 };
 
 enum class SensorEventTypeCodes : uint8_t
 {
     unspecified = 0x00,
     threshold = 0x01,
-    sensorSpecified = 0x6f
